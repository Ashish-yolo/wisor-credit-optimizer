{
  "name": "wisor-credit-optimizer",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "npm run build:css && mkdir -p dist && cp public/index.html dist/ && cp assets/favicon.png dist/ && cp public/_redirects dist/ && cp public/*.zip dist/",
    "build:css": "tailwindcss -i ./src/input.css -o ./public/styles.css --minify",
    "dev:css": "tailwindcss -i ./src/input.css -o ./public/styles.css --watch",
    "build:mobile": "expo export --platform web --output-dir dist"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/native": "^7.1.17",
    "expo": "~53.0.20",
    "expo-barcode-scanner": "^13.0.1",
    "expo-camera": "^16.1.11",
    "expo-linear-gradient": "^14.1.5",
    "expo-status-bar": "~2.2.3",
    "lucide-react-native": "^0.539.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.79.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.2.0",
    "typescript": "~5.8.3",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31"
  },
  "private": true
}
