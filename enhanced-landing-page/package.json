{
  "name": "wisor-enhanced-landing",
  "version": "1.0.0",
  "description": "Enhanced Wisor landing page with optimizations and accessibility",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
